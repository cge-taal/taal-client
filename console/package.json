{
  "name": "taal-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 8080",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@storybook/addon-essentials": "^7.0.0-alpha.62",
    "@storybook/addon-interactions": "^7.0.0-alpha.62",
    "@storybook/addon-links": "^7.0.0-alpha.62",
    "@storybook/blocks": "^7.0.0-alpha.62",
    "@storybook/svelte": "^7.0.0-alpha.62",
    "@storybook/sveltekit": "^7.0.0-alpha.62",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@zerodevx/svelte-toast": "^0.8.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-alpha.62",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "focus-visible": "^5.2.0",
    "i18next": "^22.0.6",
    "mime": "^3.0.0",
    "polished": "^4.2.2",
    "svelte-i18next": "^1.2.2"
  }
